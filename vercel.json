{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "functions": {
    "api/**/*.js": {
      "maxDuration": 120
    }
  },
  "github": {
    "silent": false
  },
  "rewrites": [
    {
      "source": "/api/analyze-brand-enhanced",
      "destination": "/api/analyze-brand-enhanced"
    },
    {
      "source": "/api/extract-brand",
      "destination": "/api/extract-brand"
    },
    {
      "source": "/api/discover-brand-pages",
      "destination": "/api/discover-brand-pages"
    },
    {
      "source": "/health",
      "destination": "/api/health"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}